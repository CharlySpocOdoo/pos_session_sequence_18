<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record model="ir.ui.view" id="res_config_settings_view_form_inherit_pos_session_sequence">
        <field name="name">res.config.settings.inherit.pos.session.sequence</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="point_of_sale.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@id='pos_interface_section']" position="inside">
                <setting help="Generation of your sessions references">
                    <field name="pos_session_sequence_id"/>
                </setting>
            </xpath>
        </field>
    </record>
</odoo>
